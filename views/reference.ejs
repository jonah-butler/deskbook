<%- include ('partials/header') %>


<div class="view-container">
  <div class="empty-view">
  </div>
  <div class="container-fluid data-reference">
    <ul class="list-group">

    </ul>
  </div>
</div>

<footer class="footer-container slate-grey">
  <div class="container-fluid color-white">
    <div class="row">
      <div class="flex-container-row">
    <div class="">
      <% const today = new Date();%>
      <% const yesterday = new Date();%>
      <% const weekAgo = new Date(Date.now() - 604800000); %>
      <button class="icon-circle slate-grey"><i class="fas fa-search"></i></button>
      <input id="date-input-start" class="slate-grey" type="text" value="<%= `${today.getMonth() + 1}/${today.getDate()}/${today.getFullYear()}` %>" placeholder="From">
      <i class="fas fa-angle-double-right"></i>
      <input id="date-input-end" class="slate-grey" value="<%= `${today.getMonth() + 1}/${today.getDate()}/${today.getFullYear()}` %>" type="text" placeholder="To">
    </div>
    <!-- Default dropup button -->
    <div class="dropup padding-sm">
      <button class="btn btn-default dropdown-toggle" type="button" id="branchDropdown" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
        All<span class="caret"></span>
      </button>
      <ul class="dropdown-menu" aria-labelledby="dropdownMenu2">
        <li>All</>
        <li>Belmont</li>
        <li>Broad Rock</li>
        <li>East End</li>
        <li>Ginter Park</li>
        <li>Hull Street</li>
        <li>Main</li>
        <li>North Avenue</li>
        <li>West End</li>
        <li>Westover Hills</li>
      </ul>
    </div>
    <div class="padding-sm">
      <button class="btn btn-default dropdown-toggle" id="today" type="button">
        Today
      </button>
      <button class="btn btn-default dropdown-toggle" id="tomorrow" type="button">
        Yesterday
      </button>
    </div>
    </div>
    </div>
  </div>
</footer>


<%- include ('partials/footer') %>
<script type="module" src="/reference.js"></script>
